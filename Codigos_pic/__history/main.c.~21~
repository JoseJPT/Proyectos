#include <16F877A.h>
#fuses XT, NOWDT
#USE DELAY(CLOCK=4M) 
#use rs232(baud=9600, xmit=PIN_C6, rcv=PIN_C7)

#define A0 PIN_A0
#define D1 PIN_D1
#define D0 PIN_D0
#define A1 PIN_A1

#define D2 PIN_D2
#define D3 PIN_D3
#define B5 PIN_B5
#define B6 PIN_B6

void main(){
output_high(PIN_A1);
delay (5000);

output_low(PIN_A1);
delay (4000);
}


